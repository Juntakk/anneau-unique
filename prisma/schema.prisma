generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
   id String @id 

   PlayerInfo PlayerInfo[]
}

model PlayerInfo{
   id String @id
   playerId String @unique
   player Player @relation(fields: [playerId], references: [id])
   culture String
}